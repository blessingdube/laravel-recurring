{
  "name": "blessingdube/laravel-recurring",
  "description": "Package for adding recurring functionality to laravel models.",
  "keywords": [
    "blessingdube",
    "laravel-recurring"
  ],
  "homepage": "https://github.com/blessingdube/laravel-recurring",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "illuminate/database": "^7.0",
    "nesbot/carbon": "^2.17"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "BlessingDube\\Recurring\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BlessingDube\\Recurring\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "BlessingDube\\Recurring\\RecurringServiceProvider"
      ]
    }
  }
}
